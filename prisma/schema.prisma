// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Person {
  id        String @id @default(uuid())
  firstName String
  lastName  String?
  gender    String?
  birthdate DateTime?
  phone     String?
  email     String?
  street1   String?
  street2   String?
  city      String?
  state     String?
  zip       String?
  createdAt DateTime @default(now()) @db.Timestamptz()
  student   Student?
}
